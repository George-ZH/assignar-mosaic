{
  "name": "hipage-test",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "prop-types": "^15.5.10",
    "react": "^15.5.4",
    "react-dom": "^15.5.4",
    "react-paginate": "^4.4.2",
    "react-redux": "^5.0.5"
  },
  "devDependencies": {
    "babel": "^6.23.0",
    "babel-jest": "^20.0.3",
    "babel-loader": "^7.0.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react-native": "^1.9.2",
    "css-loader": "^0.28.4",
    "enzyme": "^2.8.2",
    "jest": "^20.0.4",
    "jest-cli": "^20.0.4",
    "jest-css-modules": "^1.1.0",
    "jest-fetch-mock": "^1.1.1",
    "react-addons-test-utils": "^15.5.1",
    "react-scripts": "1.0.7",
    "react-test-renderer": "^15.5.4",
    "redux": "^3.6.0",
    "redux-mock-store": "^1.2.3",
    "redux-thunk": "^2.2.0",
    "webpack": "^2.6.1"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "webpack",
    "test": "jest",
    "test:watch": "jest --watch"
  },
  "jest": {
    "transform": {
      ".*": "<rootDir>/node_modules/babel-jest"
    },
    "setupFiles": [
      "./setupJest.js"
    ],
    "moduleNameMapper": {
      "\\.(css)$": "<rootDir>/node_modules/jest-css-modules",
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga|yml)$": "<rootDir>/src/mock/fileMock.js"
    },
    "unmockedModulePathPatterns": [
      "react",
      "react-dom",
      "react-addons-test-utils",
      "fbjs",
      "enzyme",
      "cheerio",
      "htmlparser2",
      "underscore",
      "lodash",
      "domhandler",
      "object.assign",
      "define-properties",
      "function-bind",
      "object-keys"
    ]
  }
}
